{
  "name": "shop",
  "version": "1.0.0",
  "description": "Shop service for online-store project",
  "main": "./src/index.ts",
  "scripts": {
    "build": "rimraf ./build && tsc && cp ./src/db.bkp.json ./build/db.json && cp ./src/db.bkp.json ./build/db.bkp.json && cp .env ./build && cp ./package.json ./package-lock.json ./build && cd build && npm pkg delete scripts && npm pkg set scripts.start='node -r dotenv/config index.js'",
    "start": "ts-node-dev -r dotenv/config src/index.ts",
    "test": "jest",
    "test:watch": "npm test -- --watchAll --no-cache",
    "postinstall": "cp -n .example.env .env || true"
  },
  "keywords": [],
